{
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "stage-0"
    ],
    "plugins": [
        [
          "transform-react-jsx", {
            "pragma": "h"
          }
        ]
    ],
    "env": {
      "commonjs": {
        "plugins": [
          "add-module-exports",
          [
            "transform-es2015-modules-commonjs",
            {
              "loose": true,
              "strict": false
            }
          ]
        ]
      }
    }
}
